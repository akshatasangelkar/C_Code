#include <stdio.h>
#define SIZE 10
int main() {
  int even=0,odd=0,arr[SIZE];
  printf("Enter the numbers : ");
  for(int i=0;i<SIZE;i++)
  {
      scanf("%d",&arr[i]);
      if(arr[i]%2==0)
      {
          even++;
      }
      else
      {
          odd++;
      }
  }
  printf("array elements are : ");
  for(int i=0;i<SIZE;i++)
  {
      printf("%d\t",arr[i]);
  }
  printf("\n");
  printf("even number count : %d\n",even);
  printf("odd number count : %d",odd);
  return 0;
}
