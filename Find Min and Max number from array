#include <stdio.h>
int main() {
    int arr[]={2,6,9,1,0,100,78,54,77,-1};
    int min,max;
    min=max=arr[0];
    for(int i=1;i<10;i++)
    {
        if(arr[i]<min)
        {
            min=arr[i];
        }
         if(arr[i]>max)
        {
            max=arr[i];
        }
    } 
    printf("Min number is %d \n",min);
    printf("Max number is %d \n",max);

  return 0;
}
